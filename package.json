{
  "name": "trail-client",
  "description": "Distributed tracing client for Node.js",
  "repository": "CatTail/node-trail-client",
  "version": "0.2.0",
  "main": "dist/index.js",
  "keywords": [],
  "files": [
    "dist"
  ],
  "devDependencies": {
    "babel-cli": "^6.7.7",
    "babel-eslint": "^6.0.4",
    "babel-plugin-transform-async-to-generator": "^6.8.0",
    "babel-plugin-transform-es2015-destructuring": "^6.8.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.8.0",
    "babel-plugin-transform-es2015-parameters": "^6.8.0",
    "chai": "^3.5.0",
    "dnode": "^1.2.2",
    "eslint-plugin-babel": "^3.2.0",
    "istanbul": "^0.4.3",
    "mocha": "^2.4.5",
    "redis": "^0.12.1",
    "sinon": "^1.17.4",
    "sinon-chai": "^2.8.0"
  },
  "license": "MIT",
  "dependencies": {
    "basictracer": "^0.4.0",
    "continuation-local-storage": "^3.1.7",
    "debug": "^2.2.0"
  },
  "scripts": {
    "debug": "mocha --debug-brk $npm_package_options_mocha",
    "build": "babel src -d dist --copy-files",
    "watch": "babel src -d dist --copy-files -w",
    "lint": "eslint .",
    "test": "NODE_ENV=test mocha --reporter spec $npm_package_options_mocha 'dist/**/*.spec.js' 'dist/**/*.spec.e2e.js'",
    "test-cov": "NODE_ENV=test istanbul cover --root dist --report html _mocha -- $npm_package_options_mocha 'dist/**/*.spec.js' 'dist/**/*.spec.e2e.js'",
    "test-travis": "NODE_ENV=test istanbul cover --root dist --report lcovonly _mocha -- $npm_package_options_mocha 'dist/**/*.spec.js' 'dist/**/*.spec.e2e.js'",
    "preversion": "npm run build"
  },
  "options": {
    "mocha": "--timeout 5000 --require ./resources/mocha-bootload --check-leaks --full-trace resources/setup.js"
  }
}
